#Select information for which routine is making
<SelectInformationStd>
    name:'selectInfo'
    MDBoxLayout:
        orientation: "vertical"
		size: root.width, root.height

        #header
        MDGridLayout:
            cols:3
            size_hint_y: None
            height: self.minimum_height
            spacing:50

            MDRectangleFlatButton:
                text:'Home'
                #font_size:32
                on_release:
				    app.root.current = "main"
				    root.manager.transition.direction = "left"

            MDLabel:
                text: "Select information"
                font_size: 50
                halign:'center'
                color:'#ff3333'


        #Dummy to make space
        MDGridLayout:
            cols:1
            padding:100
            MDLabel:
                text:""

        #content
        MDGridLayout:
            cols:2
            size_hint:0.7,0.5
            pos_hint:{'center_x':0.5,'center_y':0.5}
            spacing:50

            MDGridLayout:
                cols:2
                MDLabel:
                    text:'Year :'
                    font_size:25
                Spinner:
                    id:year
                    text:'Select'
                    font_size:25
                    values:['1st','2nd','3rd','4th']

            MDGridLayout:
                cols:2
                MDLabel:
                    text:'Semester :'
                    font_size:25
                Spinner:
                    id:sem
                    text:'Select'
                    font_size:25
                    values:['1st','2nd']

            MDGridLayout:
                cols:2
                MDLabel:
                    text:'Section :'
                    font_size:25
                Spinner:
                    id:sec
                    text:'Select'
                    font_size:25
                    values:['A','B']

        MDGridLayout:
            cols:3
            size_hint_y: None
            height: self.minimum_height
            spacing:50
            padding:50

            MDLabel:
                text:''
            MDRectangleFlatButton:
                text:'Select'
                #font_size:32
                on_release:
		            app.loadMakeWindow(root.ids.year.text,root.ids.sem.text,root.ids.sec.text,'left')
            MDLabel:
                text: ""

        #dummy
        MDGridLayout:
            cols:1
            size_hint_y: None
            height: self.minimum_height
            padding:50
            MDLabel:
                text:' '





