#main window panel
<MainWindow>:
	name: "main"

	MDBoxLayout:
		orientation: "vertical"
		size: root.width, root.height

        #header
        MDGridLayout:
		    cols:2
		    spacing:10
		    MDGridLayout:
		        cols:1
		        size_hint_x:None
		        width:300
		        padding:0
		        MDGridLayout:
		            cols:2
		            Label:
		                text:''
                    Image:
                        source:'File\\Image\\DUET.png'
                        allow_stretch: True
                        keep_ratio: True
                        size_hint_y: None
                        size_hint_x: None
                        width: 150 #self.parent.width
                        height: 150/self.image_ratio
                        pos_hint_y:0.2
                Label:
                    text:''
		    MDGridLayout:
		        cols:1
		        size_hint_y:None
		        height:self.minimum_height+150
		        MDLabel:
		            text:'Dhaka University of Engineering & Technology, Gazipur'
		            font_size:40
		        MDLabel:
		            text:'Department of Computer Science and Engineering'
		            font_size:35

		Label:
		    text:''

		MDLabel:
			text: "Routine Management System"
			font_size: 80
			halign: "center"
			color:'#ff3333'

        MDGridLayout:
            cols:3

            spacing:50
            padding:100

            MDRectangleFlatButton:
                text:'Course Coordinator Panel'
                size_hint_x:0.3
                font_size:28
                on_release:
                    app.root.current = "courseCo"
			        root.manager.transition.direction = "right"

            MDRectangleFlatButton:
                text:'Teachers Panel'
                size_hint_x:0.3
                font_size:28
                on_release:
                    app.root.current = "teachersP"
			        root.manager.transition.direction = "right"

            MDRectangleFlatButton:
                text:'Student Panel'
                size_hint_x:0.3
                font_size:28
                on_release:
                    app.root.current = "stdpanel"
			        root.manager.transition.direction = "left"

		Label:
		    text:''



