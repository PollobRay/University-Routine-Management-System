
<Notification>
    MDRectangleFlatButton:
        text:'OK'
        text_color:"#e7040e"
        on_release:
            root.dismiss()


#popup window
<PopWidget>
    title:"Schedule class"
    size_hint:None, None
    size:400, 400
    title_align : 'center'
    title_color : '#d82f05'
    title_size : '26sp'

    GridLayout:
        cols:1
        GridLayout:
            cols:2
            Label:
                text:'Course Code :'
            TextInput:
                id:courseCode
                text:root.getCourseCode() #initialized

        GridLayout:
            cols:2
            Label:
                text:'Course Title :'
            TextInput:
                id:courseName
                text:root.getCourseName() #initialized by if before filled values

        GridLayout:
            cols:2
            Label:
                text:'Course Teacher :'
            Spinner:
                id:teachers1
                text:root.getTeacherName1()
                values:root.getTeacherList()
                on_text: root.spinnerEvent1()

        #Teacher 2
        GridLayout:
            cols:2
            Label:
                text:'Course Teacher 2 :'
                disabled: not Select.active
            Spinner:
                id:teachers2
                text:root.getTeacherName2()
                values:root.getTeacherList()
                disabled: not Select.active
                on_text: root.spinnerEvent2()

        GridLayout:
            cols:2
            Label:
                text:'Select ClassRoom :'

            Spinner:
                id:rooms
                text:root.getRoomNo()
                values:root.getRoomList() #['7019 A','7019 B']
                on_text: root.spinnerEvent3()

        GridLayout:
            cols:4
            Label:
                text:''
            CheckBox:
                id:Select
                active: root.isLab()

            Label:
                text:'Lab Class'

            Label:
                text:''

        GridLayout:
            cols:3
            Button:
                id: sdlBtn
                text:'Schedule'
                on_release:root.press_btn()

            Label:
                text:''

            Button:
                text:'close'
                on_release:root.dismiss()

