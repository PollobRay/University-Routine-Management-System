            spacing:100

		    MDGridLayout:
		        cols: 2
		        MDLabel:
		            text:'Year :'
		            font_size:25

		        Spinner:
		            id:year
		            text:'<select>'
		            font_size:25
		            values: ['1st','2nd','3rd','4th']

		    MDGridLayout:
		        cols: 2
		        MDLabel:
		            text:'Semester :'
		            font_size:25
		        Spinner:
		            id:sem
		            text:'<select>'
		            font_size:25
		            values: ['1st','2nd']

		    MDGridLayout:
		        cols: 2
		        MDLabel:
